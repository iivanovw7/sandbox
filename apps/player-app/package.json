{
    "name": "player-app",
    "version": "1.0.0",
    "author": "iivanovw7",
    "private": true,
    "description": "OTT media service website",
    "directories": {
        "assets": "assets",
        "types": "types",
        "config": "config",
        "build": "build",
        "src": "src",
        "test": "test"
    },
    "main": "src/main.tsx",
    "scripts": {
        "build": "NODE_ENV=production pnpm vite build",
        "build:analyze": "pnpm vite build --mode analyze",
        "preview": "pnpm run build && vite preview",
        "clean": "rimraf .turbo node_modules build",
        "dev": "pnpm vite",
        "doc": "typedoc",
        "extract-messages": "formatjs extract 'src/**/*.ts' --ignore='**/*.d.ts' --ignore='**/*.css.ts' --out-file src/shared/translations/lang.json",
        "compile-messages": "formatjs compile 'src/shared/translations/lang.json' --out-file src/shared/translations/en.json",
        "create-translations": "npm run extract-messages && npm run compile-messages && rimraf src/shared/translations/lang.json",
        "lint": "eslint --cache '{src,test}/**/*.{ts,tsx}'",
        "lint:fix": "eslint --cache --max-warnings 0 '{src,test}/**/*.{ts,tsx}' --fix",
        "reinstall": "pnpm run clean && pnpm run bootstrap",
        "test": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest watch"
    },
    "devDependencies": {
        "@babel/core": "7.21.4",
        "@babel/eslint-parser": "7.21.3",
        "@babel/eslint-plugin": "7.19.1",
        "@babel/preset-env": "7.21.4",
        "@babel/preset-typescript": "7.21.4",
        "@formatjs/cli": "6.1.1",
        "@sandbox/eslint-config": "workspace:*",
        "@sandbox/player-app-server": "workspace:*",
        "@sandbox/ts-config": "workspace:*",
        "@sandbox/types": "workspace:*",
        "@solid-devtools/overlay": "0.27.4",
        "@solidjs/testing-library": "0.8.4",
        "@types/jsdom": "21.1.1",
        "@types/node": "18.16.16",
        "@types/ramda": "0.29.2",
        "@vanilla-extract/vite-plugin": "3.8.2",
        "@vitejs/plugin-basic-ssl": "1.0.1",
        "@vitest/coverage-istanbul": "0.32.2",
        "autoprefixer": "10.4.14",
        "browserslist": "4.21.7",
        "eslint": "8.43.0",
        "jsdom": "22.1.0",
        "miragejs": "0.1.47",
        "postcss": "8.4.24",
        "postcss-100vh-fix": "1.0.2",
        "postcss-normalize": "10.0.1",
        "postcss-preset-env": "8.5.1",
        "rimraf": "5.0.1",
        "rollup-plugin-visualizer": "5.9.2",
        "typedoc": "0.24.8",
        "typedoc-theme-hierarchy": "3.2.0",
        "typescript": "5.0.4",
        "unplugin-auto-import": "0.16.4",
        "vite": "4.3.9",
        "vite-imagetools": "4.0.19",
        "vite-plugin-compression": "0.5.1",
        "vite-plugin-dynamic-import": "1.4.1",
        "vite-plugin-eslint": "1.8.1",
        "vite-plugin-fonts": "0.7.0",
        "vite-plugin-html": "3.2.0",
        "vite-plugin-solid": "2.7.0",
        "vite-plugin-static-copy": "0.16.0",
        "vite-plugin-svg-icons": "2.0.1",
        "vitest": "0.32.0"
    },
    "dependencies": {
        "@cookbook/solid-intl": "0.1.2",
        "@kobalte/core": "0.9.6",
        "@kobalte/vanilla-extract": "0.4.0",
        "@solid-primitives/storage": "1.3.11",
        "@solid-primitives/utils": "6.2.0",
        "@solidjs/router": "0.8.2",
        "@vanilla-extract/css": "1.11.1",
        "@vanilla-extract/css-utils": "0.1.3",
        "@vanilla-extract/dynamic": "2.0.3",
        "@vanilla-extract/recipes": "0.4.0",
        "axios": "1.4.0",
        "dashjs": "4.7.1",
        "loglevel": "1.8.1",
        "polished": "4.2.2",
        "ramda": "0.29.0",
        "ramda-adjunct": "4.0.0",
        "solid-js": "1.7.5",
        "solidjs-use": "2.2.0",
        "uuid": "9.0.0",
        "yup": "1.2.0"
    }
}

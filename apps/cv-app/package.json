{
    "name": "cv-app",
    "author": "iivanovw7",
    "private": true,
    "scripts": {
        "build": "qwik build",
        "build.client": "vite build",
        "build.preview": "vite build --ssr src/entry.preview.tsx",
        "build.server": "vite build -c ./config/vite-config/config.vercel.ts",
        "build.types": "tsc --incremental --noEmit",
        "deploy": "vercel deploy",
        "dev": "vite --mode ssr",
        "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
        "lint": "eslint 'src/**/*.ts*'",
        "preview": "qwik build preview && vite preview --open",
        "start": "vite --open --mode ssr",
        "qwik": "qwik"
    },
    "dependencies": {
        "@qwikest/icons": "0.0.8",
        "pixi.js": "7.2.4",
        "gsap": "3.12.2"
    },
    "devDependencies": {
        "@builder.io/partytown": "^0.8.0",
        "@builder.io/qwik": "1.2.6",
        "@builder.io/qwik-city": "1.2.6",
        "@sandbox/eslint-config": "workspace:*",
        "@sandbox/ts-config": "workspace:*",
        "@sandbox/types": "workspace:*",
        "@sandbox/utils": "workspace:*",
        "@types/eslint": "8.44.0",
        "@types/node": "18.17.0",
        "@typescript-eslint/eslint-plugin": "5.62.0",
        "@typescript-eslint/parser": "5.62.0",
        "autoprefixer": "10.4.14",
        "eslint": "8.39.0",
        "eslint-plugin-qwik": "0.107.0",
        "postcss": "8.4.27",
        "postcss-100vh-fix": "1.0.2",
        "postcss-normalize": "10.0.1",
        "postcss-preset-env": "8.5.1",
        "stylelint": "15.10.2",
        "stylelint-config-recommended": "13.0.0",
        "stylelint-config-tailwindcss": "0.0.7",
        "tailwindcss": "3.3.3",
        "typescript": "5.1.6",
        "undici": "5.22.1",
        "unplugin-auto-import": "0.16.4",
        "vercel": "29.4.0",
        "vite": "4.4.6",
        "vite-tsconfig-paths": "4.2.0"
    }
}
